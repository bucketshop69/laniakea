{
  "name": "laniakea",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:api": "pnpm --filter api dev",
    "dev:facilitator": "pnpm --filter facilitator dev",
    "demo": "pnpm --filter client-demo start",
    "install:all": "pnpm install && pnpm --recursive install",
    "dev:kora": "echo 'Please run Kora separately: cd kora-source && kora rpc start --config ../kora/kora.toml --rpc-url https://api.devnet.solana.com' && exit 1",
    "dev:kora-rust": "echo 'Please run Kora from Rust source: cd kora-source && cargo run --bin kora -- --config ../kora/kora.toml --rpc-url https://api.devnet.solana.com rpc start' && exit 1",
    "dev:all": "echo 'Starting all services (make sure Kora is already running)...' && ./start.sh"
  },
  "workspaces": [
    "api",
    "facilitator",
    "client-demo"
  ],
  "dependencies": {
    "@drift-labs/sdk": "^2.142.0-beta.13",
